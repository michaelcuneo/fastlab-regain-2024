{
	"name": "sveltekit",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "sst bind vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "npm run smui-theme",
		"smui-theme": "smui-theme compile static/smui.css -i src/theme",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint ."
	},
	"devDependencies": {
		"@smui/button": "^8.0.0-beta.3",
		"@smui/card": "^8.0.0-beta.3",
		"@smui/circular-progress": "^8.0.0-beta.3",
		"@smui/common": "^8.0.0-beta.3",
		"@smui/dialog": "^8.0.0-beta.3",
		"@smui/form-field": "^8.0.0-beta.3",
		"@smui/icon-button": "^8.0.0-beta.3",
		"@smui/layout-grid": "^8.0.0-beta.3",
		"@smui/paper": "^8.0.0-beta.3",
		"@smui/segmented-button": "^8.0.0-beta.3",
		"@smui/slider": "^8.0.0-beta.3",
		"@smui/switch": "^8.0.0-beta.3",
		"@smui/textfield": "^8.0.0-beta.3",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"chart.js": "^4.4.7",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"runed": "^0.23.1",
		"sanitize.css": "^13.0.0",
		"smui-theme": "^8.0.0-beta.3",
		"svelte": "^5.7.0",
		"svelte-check": "^4.0.0",
		"svelte-kit-sst": "^2.43.5",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.0.3"
	},
	"dependencies": {
		"melt": "^0.3.0"
	}
}
